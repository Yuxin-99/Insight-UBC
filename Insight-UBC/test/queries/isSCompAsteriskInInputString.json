{
    "title": "Testing IS Field with Asterisk in input string",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "*ma*th*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 99
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg",
                "courses_instructor",
                "courses_title",
                "courses_pass",
                "courses_fail",
                "courses_audit",
                "courses_uuid",
                "courses_year"
            ],
            "ORDER": "courses_id"
        }
    },
    "isQueryValid": false,
    "result": "InsightError"
}
